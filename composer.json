{
    "name": "limych/sleepingowl-coreui",
    "description": "CoreUI Dashboard Template for SleepingOwl Admin Laravel Framework",
    "type": "library",
    "require": {
        "php": ">=5.6.4",
        "laravelrus/sleepingowl": "4.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Limych/sleepingowl-coreui/issues",
        "source": "https://github.com/Limych/sleepingowl-coreui"
    },
    "authors": [
        {
            "name": "Andrey \"Limych\" Khrolenok",
            "email": "andrey@khrolenok.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Limych\\SleepingOwlCoreUI\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Limych\\SleepingOwlCoreUI\\Providers\\CoreUiServiceProvider"
            ]
        }
    }
}
